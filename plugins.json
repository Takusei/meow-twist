{
  "gh": {
    "url": "https://github.com/search?q={{query}}",
    "description": "Search GitHub",
    "icon": "github.png"
  },
  "code": {
    "exec": "code",
    "description": "Open VS Code",
    "icon": "vscode.png"
  },
  "notepad": {
    "exec": "notepad",
    "description": "Open Notepad",
    "icon": "notepad.png"
  },
  "edge": {
    "exec": "start msedge",
    "description": "Open Microsoft Edge",
    "icon": "edge.png"
  },
  "chrome": {
    "exec": "start chrome",
    "description": "Open Google Chrome",
    "icon": "chrome.png"
  },
  "powershell": {
    "exec": "start powershell",
    "description": "Open PowerShell",
    "icon": "powershell.png"
  },
  "explorer": {
    "exec": "explorer",
    "description": "Open File Explorer",
    "icon": "explorer.png"
  },
  "outlook": {
    "exec": "start outlook",
    "description": "Open Microsoft Outlook",
    "icon": "outlook.png"
  },
  "youtube": {
    "url": "https://www.youtube.com",
    "description": "Open YouTube",
    "icon": "link.png"
  },
  "youtube-edge": {
    "exec": "start msedge https://www.youtube.com",
